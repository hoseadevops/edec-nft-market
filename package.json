{
  "name": "edec-nft-market",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "doc": "yarn run hardhat docgen",
    "deploy1": "npx hardhat run scripts/deployMarketRegistry.js",
    "deploy2": "npx hardhat run scripts/deployMarketTokenTransferProxy.js",
    "deploy3": "npx hardhat run scripts/deployMarketExchange.js",
    "deploy4": "npx hardhat run scripts/deployMarketExchangeWrap.js",
    "config": "npx hardhat run scripts/configMarket.js",
    "verify": "npx hardhat verify",
    "deploy:bsctest": "yarn run deploy1 --network bsctest && yarn run deploy2 --network bsctest && yarn run deploy3 --network bsctest && yarn run config --network bsctest",
    "deploy:goerli": "yarn run deploy1 --network goerli && yarn run deploy2 --network goerli && yarn run deploy3 --network goerli && yarn run config --network goerli",
    "computeAddress": "npx hardhat run scripts/contractAddress.js",
    "echo": "echo '--- --- --- --- --- --- --- --- --- --- --- --- --- --- --- ---'",
    "compile": "npx hardhat compile",
    "deploy:mock": "npx hardhat run scripts/deployMock.js --network localhost",
    "deploy:market": "yarn run deploy1 --network localhost && yarn run deploy2 --network localhost && yarn run deploy3 --network localhost && yarn run deploy4 --network localhost && yarn run config --network localhost",
    "test:fixture": "npx hardhat run scripts/fixture.js --network localhost",
    "test:market": "npx hardhat run scripts/market.js --network localhost",
    "test:market:wrap": "npx hardhat run scripts/marketWrap.js --network localhost",
    "reset": "yarn run compile && yarn run deploy:mock && yarn run deploy:market && yarn run test:fixture"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.7.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.8",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2",
    "hardhat": "^2.11.2",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "pino": "^8.7.0",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4",
    "web3": "^1.8.0",
    "xdeployer": "^1.1.25"
  },
  "dependencies": {
    "solidity-docgen": "^0.6.0-beta.29"
  }
}
